{
"manifest_version": 2,
    "name": "Wink",
    "description": "It's pick me up time!",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "src/content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "src/popup.html",
        "default_icon": "icon/original.png"
    }
}